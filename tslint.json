{
    "extends": "tslint:latest",
    "rules": {
        "interface-name": false,
        "max-line-length": [
            true,
            140
        ],
        "member-ordering": [
            true,
            "public-before-private",
            "static-before-instance",
            "variables-before-functions"
        ],
        "no-console": false,
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single"
        ]
    }
}
